

<br>
<div class="container">
	<table class="table table-hover">
	<thead>
		<tr>
			<th>Arriving From</th>
			<th>Time</th>
			<th>Carriages</th>
			<th>Other</th>
		</tr>
	</thead>
	<tbody>
		<% %>
		<% @traindata.each do |train| %>
		<tr>
			<td><%= train['origin'].first['locationName'] %></td>
			<td><%= train['eta'] %></td>
			<td><%= generate_carriage_view train['carriages'] %></td>
			<td><%= generate_svg_train_view train['carriages'] %></td>
		</tr>
		<% end %>
	</tbody>
</table></div>

<img src="/img/train-left-layered.svg" class="img-responsive" alt="Image">
